<UserControl x:Class="Aml.Editor.Plugin.Sandbox.Views.AMLLibraryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:aml="http://www.automationml.org/amlTool/"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Aml.Editor.Plugin.Sandbox.Views"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <aml:AMLTreeView Grid.Row="1" TreeViewModel="{Binding}"></aml:AMLTreeView>
    </Grid>
</UserControl>